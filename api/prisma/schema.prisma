generator client {
  provider = "prisma-client"
  output   = "../node_modules/.prisma/client"

}

datasource db {
  provider = "postgresql"
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  name      String
  password  String
  createdAt DateTime @default(now()) @map("created_at")

  @@map("users")
}
